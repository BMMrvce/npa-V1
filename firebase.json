{
  "hosting": {
    "public": "frontend/dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/make-server-60660975/**",
        "run": {
          "serviceId": "npa-backend",
          "region": "asia-south1"
        }
      },
      {
        "source": "/devices",
        "run": {
          "serviceId": "npa-backend",
          "region": "asia-south1"
        }
      },
      {
        "source": "/devices/**",
        "run": {
          "serviceId": "npa-backend",
          "region": "asia-south1"
        }
      },
      {
        "source": "/organizations",
        "run": {
          "serviceId": "npa-backend",
          "region": "asia-south1"
        }
      },
      {
        "source": "/organizations/**",
        "run": {
          "serviceId": "npa-backend",
          "region": "asia-south1"
        }
      },
      {
        "source": "/technicians",
        "run": {
          "serviceId": "npa-backend",
          "region": "asia-south1"
        }
      },
      {
        "source": "/technicians/**",
        "run": {
          "serviceId": "npa-backend",
          "region": "asia-south1"
        }
      },
      {
        "source": "/maintenance",
        "run": {
          "serviceId": "npa-backend",
          "region": "asia-south1"
        }
      },
      {
        "source": "/maintenance/**",
        "run": {
          "serviceId": "npa-backend",
          "region": "asia-south1"
        }
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.@(js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  }
}
